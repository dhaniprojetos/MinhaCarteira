{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",

  "BaseUrlApi": "https://localhost:44301/",
  "BaseRoutePwa":  "/",

  "DefinicaoArquivos": {
    "UploadTamanhoMaximo": "524288",
    "UploadExtensaoPermitida": ".jpg, .jpeg, .png, .svg",
    "UploadRepositorioImagens": "wwwroot/images/"
  },

  "pwa": {
    "cacheId": "Worker 1.1",
    "strategy": "cacheFirstSafe",
    //"routesToPreCache": "/Home/Contact, /Home/About",
    //"offlineRoute": "fallBack.html",
    "registerServiceWorker": true,
    "registerWebmanifest": true
  }
}
